name=inkcut
version=1.0
release=1
source=(http://downloads.sourceforge.net/project/inkcut/InkCut-1.0.tar.gz)

build() {
	cd $SRC
	mkdir -p $PKG/usr/share/inkscape/extensions
	cp -Prfd * $PKG/usr/share/inkscape/extensions/.
}
